<script src="/assets/rails-ujs.js"></script>
<%= link_to "プラン登録", new_trip_plan_path, data: { turbo: false }, class: "navi_btn" %>

<% @plans.each do |plan| %>
  <%= link_to plan.plan_title, trip_plan_path(trip_id: @trip.id, id: plan.id), data: { turbo: false }, class: "navi_btn" %>
<% end %>

<%= link_to "旅行名編集", edit_trip_path(@trip.id), data: { turbo: false }, class: "navi_btn" %>
<%= link_to "旅行全削除", trip_path(@trip.id), method: :delete, data: {confirm: "削除しますか？"} %>
